
<section id="tw-client" class="tw-client">
         <h3 class="text-center">Các nhà tài trợ</h3>
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="clients-carousel owl-carousel">
           <% sponsors.map(item => { %>
            <div class="client-logo-wrapper d-table">
              <div class="client-logo d-table-cell">
                <img src="<%- item.linkLogo %>" alt="<%- item.name %>">
              </div>
              <!-- End Clients logo -->
            </div>
            <!-- End Client wrapper -->
          <% }) %>
        </div>
        <!-- End Carousel -->
      </div>
      <!-- End Col -->
    </div>
    <!-- End Row -->
  </div>
  <!-- End Container -->
</section>
