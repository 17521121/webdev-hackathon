<div class="container" style="
    margin-bottom: calc(8vw);
    text-align: center;
    margin-top: calc(2vw);
">
    <!--Logo các nhà tài trợ-->
    <h1 style="font-size: 50px; width: 100%; font-family: Lato" >CÁC NHÀ TÀI TRỢ</h1>
    <% sponsors.map(item => { %>
      <a href='https://<%- item.link %>' target="_blank"><img src="<%- item.linkLogo %>" style="width: 80px; height: 50px; margin: 20px;" class="img-fluid ${3|rounded-top,rounded-right,rounded-bottom,rounded-left,rounded-circle,|}" alt="<%- item.name %>">
    <% }) %>
    
  </div>